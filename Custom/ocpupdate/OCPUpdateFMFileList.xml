<?xml version="1.0" encoding="utf-8"?>
<FMCollectionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.microsoft.com/embedded/2004/10/ImageUpdate" Product="IoTUAP" >
  <IsBuildFeatureEnabled>true</IsBuildFeatureEnabled>
  <FMs>
    <!-- Note: The CPUType Attribute is case Sensitive and allows values X86, AMD64 and ARM only -->
    <FM ID="OCPUpdate" Path="$(FMDirectory)\OCPUpdateFM.xml" ReleaseType="Production" OwnerName="OEM_NAME" OwnerType="OEM" CPUType="CPU_TYPE" Critical="true"/>
 </FMs>

  <SupportedResolutions>
    <Resolution>1024x768</Resolution>
  </SupportedResolutions>

  <SupportedLanguages>
    <Language>en-US</Language>
  </SupportedLanguages>

  <SupportedLocales>
    <Locale>en-US</Locale>
  </SupportedLocales>

</FMCollectionManifest>
